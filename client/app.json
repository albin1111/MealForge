{
	"expo": {
		"name": "MealForge",
		"slug": "MealForge",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/new-icon.png",
		"scheme": "MealForge",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/new-splash3.png",
			"resizeMode": "contain",
			"backgroundColor": "#151210"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/new-adaptive-icon.png",
				"backgroundColor": "#151210"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/new-favicon.png"
		},
		"plugins": [
			"expo-router",
			"expo-font"
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}